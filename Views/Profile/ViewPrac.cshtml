@model PainAssessment.ViewModels.Profile.PractionerModel
@{
    ViewData["Title"] = "ViewPrac";
}
<div>
    <h4>Profile</h4>
    <hr />
    <div class="row">
        <div class="col-lg-2"> @Html.DisplayNameFor(model => model.Name)</div>
        <div class="col-lg-10">
            @Html.TextBoxFor(model => model.Name)
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-2"> @Html.DisplayNameFor(model => model.FullName)</div>
        <div class="col-lg-10">
            @Html.TextBoxFor(model => model.FullName)
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-2"> @Html.DisplayNameFor(model => model.Role)</div>
        <div class="col-lg-10">
            @Html.TextBoxFor(model => model.Role)
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-2"> @Html.DisplayNameFor(model => model.PriorPainEducation)</div>
        <div class="col-lg-10">
            @Html.TextBoxFor(model => model.PriorPainEducation)
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-2"> @Html.DisplayNameFor(model => model.ClinicalArea)</div>
        <div class="col-lg-10">
            @Html.TextBoxFor(model => model.ClinicalArea)
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-2"> @Html.DisplayNameFor(model => model.PracticeType)</div>
        <div class="col-lg-10">
            @Html.TextBoxFor(model => model.PracticeType)
        </div>
    </div>

</div>