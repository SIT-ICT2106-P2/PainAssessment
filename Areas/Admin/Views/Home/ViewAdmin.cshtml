@model PainAssessment.Areas.Admin.Models.ViewModels.Profile.AdministratorModel
@{
    ViewData["Title"] = "ViewAdmin";
}

<div>
    <h4>Profile</h4>
    <hr />
    <div class="row">
        <div class="col-lg-2"> @Html.DisplayNameFor(model => model.Name)</div>
        <div class="col-lg-10">
            @Html.TextBoxFor(model => model.Name, new { disabled = "disabled", @readonly = "readonly" })
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-2"> @Html.DisplayNameFor(model => model.FullName)</div>
        <div class="col-lg-10">
            @Html.TextBoxFor(model => model.FullName, new { disabled = "disabled", @readonly = "readonly" })
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-2"> @Html.DisplayNameFor(model => model.Role)</div>
        <div class="col-lg-10">
            @Html.TextBoxFor(model => model.Role, new { disabled = "disabled", @readonly = "readonly" })
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-2"> @Html.DisplayNameFor(model => model.Experience)</div>
        <div class="col-lg-10">
            @Html.TextBoxFor(model => model.Experience, new { disabled = "disabled", @readonly = "readonly" })
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-2"> @Html.DisplayNameFor(model => model.ClinicalArea)</div>
        <div class="col-lg-10">
            @Html.TextBoxFor(model => model.ClinicalArea, new { disabled = "disabled", @readonly = "readonly" })
        </div>
    </div>
</div>